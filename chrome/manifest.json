{
  "manifest_version": 2,
  "name": "Autonote",
  "description": "Automatically make notes of long articles instaneously!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "http://127.0.0.1:5000/notes/"]
}
